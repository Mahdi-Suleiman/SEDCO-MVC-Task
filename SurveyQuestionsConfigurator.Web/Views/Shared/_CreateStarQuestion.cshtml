@model SurveyQuestionsConfigurator.Entities.StarQuestion

<div class="form-group">
    @Html.LabelFor(model => model.NumberOfStars, htmlAttributes: new { @class = "" })
        @Html.EditorFor(model => model.NumberOfStars, new { htmlAttributes = new { @class = "form-control", min = "1", max = "10" } })
        @Html.ValidationMessageFor(model => model.NumberOfStars, "", new { @class = "text-danger" })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
